<form action="/dashboard//suppliers/new_supplier" method="get">
    <input type="submit" value="Add New Supplier"
         name="Submit" id="frm1_submit" />
</form>

<%@suppliers.each do |supplier|  %>
  <p>Name: <%=supplier.name %></p>
  <p>Representative: <%=supplier.representative %></p>

<p> <a href="/dashboard/suppliers/<%= supplier.id %>">Show Supplier</a> </p>

<hr>
<%end  %>
